package model;

public class Edit {
	TodoFolder folder = new TodoFolder();
	//処理状態の変更
		public void editFinished(int editNo) {
			int num = editNo - 1;
			folder.getTodos().get(num).setFinished(true);
			System.out.println("こんぐらちゅれーしょん！！！");
		}
		//期限の変更
		public void editLimit() {
		}
		//優先度の変更
		public void editPrioritye() {

		}
}
